{
  "$schema": "../schemas/blocks.schema.json",
  "toolbox": "pathsim",
  "importPath": "pathsim.blocks",

  "categories": {
    "Sources": [
      { "class": "Constant", "maxInputs": 0, "maxOutputs": 1 },
      { "class": "Source", "maxInputs": 0, "maxOutputs": 1 },
      { "class": "SinusoidalSource", "maxInputs": 0, "maxOutputs": 1 },
      { "class": "StepSource", "maxInputs": 0, "maxOutputs": 1 },
      { "class": "PulseSource", "maxInputs": 0, "maxOutputs": 1 },
      { "class": "TriangleWaveSource", "maxInputs": 0, "maxOutputs": 1 },
      { "class": "SquareWaveSource", "maxInputs": 0, "maxOutputs": 1 },
      { "class": "GaussianPulseSource", "maxInputs": 0, "maxOutputs": 1 },
      { "class": "ChirpPhaseNoiseSource", "maxInputs": 0, "maxOutputs": 1 },
      { "class": "ClockSource", "maxInputs": 0, "maxOutputs": 1 },
      { "class": "WhiteNoise", "maxInputs": 0, "maxOutputs": 1 },
      { "class": "PinkNoise", "maxInputs": 0, "maxOutputs": 1 },
      { "class": "RandomNumberGenerator", "maxInputs": 0, "maxOutputs": 1 }
    ],

    "Dynamic": [
      { "class": "Integrator" },
      { "class": "Differentiator" },
      { "class": "Delay" },
      { "class": "ODE" },
      { "class": "DynamicalSystem" },
      { "class": "StateSpace" },
      { "class": "PID", "maxInputs": 1, "maxOutputs": 1 },
      { "class": "AntiWindupPID", "maxInputs": 1, "maxOutputs": 1 },
      { "class": "TransferFunctionNumDen", "maxInputs": 1, "maxOutputs": 1 },
      { "class": "TransferFunctionZPG", "maxInputs": 1, "maxOutputs": 1 },
      { "class": "ButterworthLowpassFilter" },
      { "class": "ButterworthHighpassFilter" },
      { "class": "ButterworthBandpassFilter" },
      { "class": "ButterworthBandstopFilter" }
    ],

    "Algebraic": [
      { "class": "Adder", "maxOutputs": 1 },
      { "class": "Multiplier", "maxOutputs": 1 },
      { "class": "Amplifier" },
      { "class": "Function" },
      { "class": "Sin" },
      { "class": "Cos" },
      { "class": "Tan" },
      { "class": "Tanh" },
      { "class": "Abs" },
      { "class": "Sqrt" },
      { "class": "Exp" },
      { "class": "Log" },
      { "class": "Log10" },
      { "class": "Mod" },
      { "class": "Clip" },
      { "class": "Pow" },
      { "class": "Switch", "maxOutputs": 1 },
      { "class": "LUT" },
      { "class": "LUT1D", "maxInputs": 1, "maxOutputs": 1 }
    ],

    "Mixed": [
      { "class": "SampleHold" },
      { "class": "FIR" },
      { "class": "ADC" },
      { "class": "DAC" },
      { "class": "Counter", "maxInputs": 1, "maxOutputs": 1 },
      { "class": "CounterUp", "maxInputs": 1, "maxOutputs": 1 },
      { "class": "CounterDown", "maxInputs": 1, "maxOutputs": 1 },
      { "class": "Relay", "maxInputs": 1, "maxOutputs": 1 }
    ],

    "Recording": [
      { "class": "Scope", "maxOutputs": 0 },
      { "class": "Spectrum", "maxOutputs": 0 }
    ]
  },

  "extraDocstrings": ["Subsystem", "Interface"],

  "ioOverrides": {}
}
